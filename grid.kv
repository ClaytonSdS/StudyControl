<Conteudo_Duration>
    orientation: "vertical"
    size_hint_y: None
    height: dp(200)
    spacing: 10
    MDBoxLayout:
        orientation: 'vertical'
        md_bg_color: 1,0,0,0
        pos_hint: {"center_x":0.5, "center_y": 0.5}
        size: self.parent.size
        ###
        GridLayout:
            rows: 2
            cols:1
            spacing: 10
            # BOX LAYOUT PARA TODOS OS TEXTOS + TEXT INPUTS
            MDBoxLayout:
                orientation: 'vertical'
                md_bg_color: 1,0,0,0
                size_hint: (1,1)
                spacing: 10

                # BOX LAYOUT PARA HORAS | MINUTOS ESCRITOS
                MDBoxLayout:
                    md_bg_color: 0,0,0,0
                    orientation: 'horizontal'
                    spacing: 10
                    size_hint: (1,0.4)
                    MDBoxLayout_Hover:
                        md_bg_color: 0,0,0,0
                        on_enter: self.cor = app.cor_widgets_hover
                        on_leave: self.cor = app.cor_widgets_default
                        canvas.before:
                            Color:
                                rgb:  get_color_from_hex('#{}'.format(self.cor))
                            RoundedRectangle:
                                pos: self.pos
                                size: self.size
                                radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]
                        MDLabel:
                            markup: True
                            text: '[b]Horas[/b]'
                            halign: 'center'
                            valign: 'middle'
                            color: get_color_from_hex('#{}'.format(app.cor_principal_aplicativo))
                            font_size: 20

                    MDBoxLayout_Hover:
                        md_bg_color: 0,0,0,0
                        size_hint:(0.13,1)

                    MDBoxLayout_Hover:
                        md_bg_color: 0,0,0,0
                        on_enter: self.cor = app.cor_widgets_hover
                        on_leave: self.cor = app.cor_widgets_default
                        padding: 10
                        canvas.before:
                            Color:
                                rgb:  get_color_from_hex('#{}'.format(self.cor))
                            RoundedRectangle:
                                pos: self.pos
                                size: self.size
                                radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]
                        MDLabel:
                            markup: True
                            text: '[b]Minutos[/b]'
                            halign: 'center'
                            valign: 'middle'
                            color: get_color_from_hex('#{}'.format(app.cor_principal_aplicativo))
                            font_size: 20
                #///////////////////////////////////////////////////
                # BOX PARA BOTAO HORAS | DOIS PONTOS | BOTAO MINUTOS
                MDBoxLayout:
                    md_bg_color: 0,0,0,0
                    orientation: 'horizontal'
                    spacing: 10
                    # BOTAO HORAS
                    MDBoxLayout_Hover:
                        md_bg_color: 0,0,0,0
                        on_enter: self.cor = app.cor_widgets_hover
                        on_leave: self.cor = app.cor_widgets_default
                        padding: 10
                        canvas.before:
                            Color:
                                rgb:  get_color_from_hex('#{}'.format(self.cor))
                            RoundedRectangle:
                                pos: self.pos
                                size: self.size
                                radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]
                        MDTextField:
                            id: horas_
                            size: self.parent.size
                            pos_hint: {"center_x":0.5, "center_y": 0.58}
                            font_size: 80
                            text: "00"
                            max_text_length: 2
                            input_filter: "int"
                            helper_text: 'Hora'
                            helper_text_mode: 'persistent'
                            line_color_focus: get_color_from_hex('#{}'.format(app.cor_principal_aplicativo))
                            text_color_focus: get_color_from_hex('#{}'.format(app.cor_principal_aplicativo))

                    # DOIS PONTOS
                    MDBoxLayout:
                        md_bg_color: 0,0,0,0
                        orientation: 'vertical'
                        spacing: 15
                        padding: [0,20]
                        size_hint: (0.13,1)
                        MDBoxLayout_Hover:
                            md_bg_color: 0,0,0,0
                            canvas.before:
                                Color:
                                    rgb:  get_color_from_hex('#{}'.format(self.cor))
                                RoundedRectangle:
                                    pos: self.pos
                                    size: self.size
                                    radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                        MDBoxLayout_Hover:
                            md_bg_color: 0,0,0,0
                            canvas.before:
                                Color:
                                    rgb:  get_color_from_hex('#{}'.format(self.cor))
                                RoundedRectangle:
                                    pos: self.pos
                                    size: self.size
                                    radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                    #///////////////////////////////////////////////////
                    # BOTAO MINUTOS
                    MDBoxLayout:
                        md_bg_color: 0,0,0,0
                        orientation: 'horizontal'
                        spacing: 10
                        MDBoxLayout_Hover:
                            md_bg_color: 0,0,0,0
                            on_enter: self.cor = app.cor_widgets_hover
                            on_leave: self.cor = app.cor_widgets_default
                            padding: 10
                            canvas.before:
                                Color:
                                    rgb:  get_color_from_hex('#{}'.format(self.cor))
                                RoundedRectangle:
                                    pos: self.pos
                                    size: self.size
                                    radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]
                            MDTextField:
                                id: minutos_
                                size: self.parent.size
                                pos_hint: {"center_x":0.5, "center_y": 0.58}
                                font_size: 80
                                text: "00"
                                max_text_length: 2
                                input_filter: "int"
