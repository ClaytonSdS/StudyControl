                                            MDLabel:
                                                markup: True
                                                text: '    Digite a duração do seu item\n[b]    Nos botões ao lado[/b]'
                                                halign: 'left'
                                                valign:'middle'
                                                color: get_color_from_hex('#686fa3')
                                                font_size: 20

                                        #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                        # BOTÃO DAS HORAS
                                        MDBoxLayout_Hover:
                                            size_hint: (0.2,1)
                                            pos_hint: 0.5, 0.5
                                            canvas.before:
                                                Color:
                                                    rgb: (self.R,self.G,self.B)
                                                RoundedRectangle:
                                                    pos: self.pos
                                                    size: self.size
                                                    radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                                            MDBoxLayout:
                                                md_bg_color: 1,0,0,0
                                                padding: 10

                                                #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                                # TEXT FIELD PARA DIGITAR AS HORAS
                                                MDTextField:
                                                    id: duracao_horas
                                                    size_hint: (0.2,1.35)
                                                    helper_text: "Hora"
                                                    max_text_length: 2
                                                    helper_text_mode: "persistent"
                                                    required: True
                                                    line_color_focus: 0.4078, 0.4352, 0.6392,1
                                                    text_color_focus: 0.4078, 0.4352, 0.6392,1
                                                    text:'00'
                                                    font_size:"60dp"
                                                    input_filter: 'int'


                                        #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                        # B0X PARA COLOCAR OS DOIS PONTOS :
                                        MDBoxLayout_Hover:
                                            size_hint: (0.1,1)
                                            pos_hint: 0.5, 0.5
                                            canvas.before:
                                                Color:
                                                    #rgba: (0.9020, 0.9216, 0.9843,0)
                                                    rgb: (self.R,self.G,self.B)
                                                RoundedRectangle:
                                                    pos: self.pos
                                                    size: self.size
                                                    radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                                            Image:
                                                source: 'icons/double_dot.png'
                                                size_hint: (0.65,0.65)
                                                pos_hint: {"center_x":0.5, "center_y": 0.5}

                                                allow_stretch: True
                                                allow_ratio: True
                                                center_x: self.parent.center_x
                                                center_y: self.parent.center_y
                                                color: get_color_from_hex('#94969d')

                                        #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                        # BOX PARA COLOCAR TEXT FIELD - DIGITE OS MINUTOS
                                        MDBoxLayout_Hover:
                                            size_hint: (0.2,1)
                                            pos_hint: 0.5, 0.5
                                            canvas.before:
                                                Color:
                                                    rgb: (self.R,self.G,self.B)
                                                RoundedRectangle:
                                                    pos: self.pos
                                                    size: self.size
                                                    radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                                            MDBoxLayout:
                                                md_bg_color: 1,0,0,0
                                                size_hint: (0.5,1)
                                                padding: 10

                                                #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                                # TEXT FIELD PARA DIGITAR OS MINUTOS
                                                MDTextField:
                                                    #pos_hint:{'center_x':.5,'center_y':.6}
                                                    size_hint: (0.2,1.35)
                                                    id: duracao_minutos
                                                    helper_text: "Minuto"
                                                    max_text_length: 2
                                                    helper_text_mode: "persistent"
                                                    required: True
                                                    line_color_focus: 0.4078, 0.4352, 0.6392,1
                                                    text_color_focus: 0.4078, 0.4352, 0.6392,1
                                                    text:'00'
                                                    font_size:"60dp"
                                                    input_filter: 'int'

                                        #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                        # BOTÃO ADICIONAR/CRIAR ITEM NO SEU CRONOGRAMA
                                        ButtonGrid:
                                            size_hint: (1,7)
                                            orientation: 'horizontal'
                                            pos_hint: 0.5, 0.5
                                            on_release: root.addToCronograma(root)
                                            canvas.before:
                                                Color:
                                                    rgb: (self.R, self.G, self.B)
                                                RoundedRectangle:
                                                    pos: self.pos
                                                    size: self.size
                                                    radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                                            MDLabel:
                                                markup: True
                                                text: '[b]Adicionar Item no Cronograma[/b]'
                                                halign: 'center'
                                                valign:'middle'
                                                color: get_color_from_hex('#686fa3')
                                                #color: get_color_from_hex('#ffffff')
                                                font_size: 25





                #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                # BARRA INFERIOR
                MDBoxLayout:
                    orientation: 'horizontal'
                    padding: 5
                    md_bg_color: 0.8078, 0.8705, 1,1
