                                                text: '    [b]Data e Horário[/b]'
                                                halign: 'left'
                                                valign:'middle'
                                                color: get_color_from_hex('#686fa3')
                                                font_size: 20

                                        # BOX PARA COLOCAR ICONE
                                        MDBoxLayout:
                                            md_bg_color: 1,0,0,0
                                            padding: 13

                                            #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                            # ICONE
                                            Image:
                                                #size_hint: (0.5,0.5)
                                                source: 'icons/cronograma_title4.png'
                                                allow_stretch: True
                                                allow_ratio: False
                                                center_x: self.parent.center_x
                                                center_y: self.parent.center_y
                                                color: get_color_from_hex('#686fa3')



                               #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                               # BOX PARA SELECIONAR DATA + SELECIONAR TEMPO
                                MDBoxLayout:
                                    orientation: 'horizontal'
                                    pos_hint: 0.5, 0.5
                                    canvas.before:
                                        Color:
                                            rgba: (0.9019, 0.9215, 0.9843,1)
                                        RoundedRectangle:
                                            pos: self.pos
                                            size: self.size
                                            radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                                    GridLayout:
                                        rows:1
                                        cols:2
                                        spacing: 10

                                        #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                        # SELECIONAR DATA - BOTÃO COM LABEL DE SELECIONAR DATA
                                        ButtonGrid:
                                            orientation: 'horizontal'
                                            pos_hint: 0.5, 0.5
                                            on_release: root.show_date_picker()
                                            canvas.before:
                                                Color:
                                                    rgb: (self.R, self.G, self.B)
                                                RoundedRectangle:
                                                    pos: self.pos
                                                    size: self.size
                                                    radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                                            # Grid Separador
                                            GridLayout:
                                                rows:1
                                                cols:2

                                                # BOX PARA COLOCAR LABEL - SELECIONAR DATA
                                                MDBoxLayout:
                                                    md_bg_color: 1,0,0,0
                                                    size_hint: (2.2,1)

                                                    #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                                    # SELECIOANR DATA - LABEL
                                                    MDLabel:
                                                        markup: True
                                                        text: '    [b]Selecionar Data[/b]'
                                                        halign: 'left'
                                                        valign:'middle'
                                                        color: get_color_from_hex('#686fa3')
                                                        font_size: 20

                                                # BOX PARA COLOCAR IMAGEM RELACIONADA A  SELECIONAR DATA
                                                MDBoxLayout:
                                                    md_bg_color: 0,0,1,0

                                                    #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                                    # ICONE SELECIONAR DATA
                                                    Image:
                                                        source: 'icons/calendar_time.png'
                                                        size_hint: 0.6,0.6
                                                        pos_hint: {'center_x': .5,'center_y': .5}
                                                        allow_stretch: True
                                                        allow_ratio: False
                                                        center_x: self.parent.center_x
                                                        center_y: self.parent.center_y
                                                        color: get_color_from_hex('#686fa3')


                                        #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                        # SELECIONAR HORÁRIO
                                        ButtonGrid:
                                            orientation: 'horizontal'
                                            pos_hint: 0.5, 0.5
                                            on_release: root.show_time_picker()
                                            canvas.before:
                                                Color:
                                                    rgb: (self.R, self.G, self.B)
                                                RoundedRectangle:
                                                    pos: self.pos
                                                    size: self.size
                                                    radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                                            # Grid Separador
                                            GridLayout:
                                                rows:1
                                                cols:2

                                                # BOX PARA COLOCAR LABEL - SELECIONAR HORÁRIO
                                                MDBoxLayout:
                                                    md_bg_color: 1,0,0,0
                                                    size_hint: (2.2,1)

                                                    #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                                    # SELECIOANR HORÁRIO - LABEL
                                                    MDLabel:
                                                        markup: True
                                                        text: '    [b]Selecionar Horário[/b]'
                                                        halign: 'left'
                                                        valign:'middle'
                                                        color: get_color_from_hex('#686fa3')
                                                        font_size: 20

                                                # BOX PARA COLOCAR IMAGEM RELACIONADA A  SELECIONAR HORÁRIO
                                                MDBoxLayout:
                                                    md_bg_color: 0,0,1,0

                                                    #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                                    # ICONE SELECIONAR HORÁRIO
                                                    Image:
                                                        source: 'icons/calendar_hour.png'
                                                        size_hint: 0.6,0.6
                                                        pos_hint: {'center_x': .5,'center_y': .5}
                                                        allow_stretch: True
                                                        allow_ratio: False
                                                        center_x: self.parent.center_x
                                                        center_y: self.parent.center_y
                                                        color: get_color_from_hex('#686fa3')



                                #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                # BOX PARA TEMPO DE ESTUDO (DURAÇÃO)
                                MDBoxLayout_Hover:
                                    size_hint: (1,0.6)
                                    orientation: 'horizontal'
                                    pos_hint: 0.5, 0.5
                                    canvas.before:
                                        Color:
                                            rgb: (self.R, self.G, self.B)
                                        RoundedRectangle:
                                            pos: self.pos
                                            size: self.size
                                            radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                                    # GRID SEPARADOR PARA LABEL + ICONE
                                    GridLayout:
                                        rows:1
                                        cols:2

                                        # BOX PARA COLOCAR TEXTO
                                        MDBoxLayout:
                                            md_bg_color: 1,0,0,0
                                            size_hint: (20,1)

                                            #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                            # TEMPO DE ESTUDO - LABEL
                                            MDLabel:
                                                markup: True
                                                text: '    [b]Tempo de Estudo (Duração)[/b]'
                                                halign: 'left'
                                                valign:'middle'
                                                color: get_color_from_hex('#686fa3')
                                                font_size: 20

                                        # BOX PARA COLOCAR ICONE
                                        MDBoxLayout:
                                            md_bg_color: 1,0,0,0
                                            padding: 13

                                            #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                            # ICONE
                                            Image:
                                                #size_hint: (0.5,0.5)
                                                source: 'icons/cronograma_title4.png'
                                                allow_stretch: True
                                                allow_ratio: False
                                                center_x: self.parent.center_x
                                                center_y: self.parent.center_y
                                                color: get_color_from_hex('#686fa3')




                               #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                               # BOX PARA SELECIONAR HORA + SELECIONAR MINUTO
                                MDBoxLayout:
                                    orientation: 'horizontal'
                                    pos_hint: 0.5, 0.5
                                    size_hint: (1,0.85)
                                    canvas.before:
                                        Color:
                                            rgba: (0.9019, 0.9215, 0.9843,1)
                                        RoundedRectangle:
                                            pos: self.pos
                                            size: self.size
                                            radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

