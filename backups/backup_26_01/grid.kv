                                    GridLayout:
                                        rows:1
                                        cols:5
                                        spacing:10

                                        #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                        # BOX PARA LABEL DIGITE AS HORAS
                                        MDBoxLayout_Hover:
                                            pos_hint: 0.5, 0.5
                                            canvas.before:
                                                Color:
                                                    rgb: (self.R,self.G,self.B)
                                                RoundedRectangle:
                                                    pos: self.pos
                                                    size: self.size
                                                    radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                                            #/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
