<Conteudo_Acertos_Erros>
    orientation: "vertical"
    size_hint_y: None
    #size_hint_x: None
    height: dp(220)
    #width: dp(70)
    #size: 70,70
    spacing: 10


    MDBoxLayout:
        orientation: 'vertical'
        md_bg_color: 1,0,0,0
        pos_hint: {"center_x":0.5, "center_y": 0.5}
        size: self.parent.size
        ###
        GridLayout:
            rows: 2
            cols:1
            spacing: 10

            # BOX LAYOUT PARA TODOS OS TEXTOS + TEXT INPUTS
            MDBoxLayout:
                orientation: 'vertical'
                md_bg_color: 1,0,0,0
                size_hint: (1,1.8)
                spacing: 10
                #///////////////////////////////////////////////////
                # BOX PARA BOTAO DE ACERTOS + TEXTO DE ACERTOS
                MDBoxLayout:
                    md_bg_color: 0,0,0,0
                    orientation: 'horizontal'
                    spacing: 10

                    # TEXTO ACERTOS:
                    MDBoxLayout_Hover:
                        on_enter: self.cor = self.cor_widgets_hover
                        on_leave: self.cor = self.cor_widgets_default
                        size_hint: (3.2,1)
                        md_bg_color: 0,0,0,0
                        #pos_hint: 0.5, 0.5
                        padding: 10
                        canvas.before:
                            Color:
                                rgb: get_color_from_hex('#{}'.format(self.cor))
                            RoundedRectangle:
                                pos: self.pos
                                size: self.size
                                radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                        MDLabel:
                            markup: True
                            text: "[b] Quantidade de Acertos:[/b]"
                            pos_hint: {"center_x":0.5, "center_y": 0.5}
                            font_size: 25
                            color: get_color_from_hex('#{}'.format(root.cor_aplicativo))

                    # BOTAO PARA ACERTOS
                    MDBoxLayout_Hover:
                        md_bg_color: 0,0,0,0
                        on_enter: self.cor = self.cor_widgets_hover
                        on_leave: self.cor = self.cor_widgets_default
                        padding: 10
                        canvas.before:
                            Color:
                                rgb:  get_color_from_hex('#{}'.format(self.cor))
                            RoundedRectangle:
                                pos: self.pos
                                size: self.size
                                radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                        MDTextField:
                            id: acertos
                            size: self.parent.size
                            pos_hint: {"center_x":0.5, "center_y": 0.58}
                            font_size: int(0.7*self.parent.width/2)
                            line_color_focus: get_color_from_hex('#{}'.format(root.cor_aplicativo))
                            text_color_focus: get_color_from_hex('#{}'.format(root.cor_aplicativo))
                            text:'0'
                            input_filter: 'int'

                #///////////////////////////////////////////////////
                # BOX PARA BOTAO DE ERROS + TEXTO DE ERROS
                MDBoxLayout:
                    md_bg_color: 0,0,0,0
                    orientation: 'horizontal'
                    spacing: 10

                    # TEXTO ERROS:
                    MDBoxLayout_Hover:
                        on_enter: self.cor = self.cor_widgets_hover
                        on_leave: self.cor = self.cor_widgets_default
                        size_hint: (3.2,1)
                        md_bg_color: 0,0,0,0
                        #pos_hint: 0.5, 0.5
                        padding: 10
                        canvas.before:
                            Color:
                                rgb: get_color_from_hex('#{}'.format(self.cor))
                            RoundedRectangle:
                                pos: self.pos
                                size: self.size
                                radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                        MDLabel:
                            markup: True
                            text: "[b] Quantidade de Erros:[/b]"
                            pos_hint: {"center_x":0.5, "center_y": 0.5}
                            font_size: 25
                            color: get_color_from_hex('#{}'.format(root.cor_aplicativo))

                    # BOTAO PARA ERROS
                    MDBoxLayout_Hover:
                        md_bg_color: 0,0,0,0
                        on_enter: self.cor = self.cor_widgets_hover
                        on_leave: self.cor = self.cor_widgets_default
                        padding: 10
                        canvas.before:
                            Color:
                                rgb:  get_color_from_hex('#{}'.format(self.cor))
                            RoundedRectangle:
                                pos: self.pos
                                size: self.size
                                radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                        MDTextField:
                            id: erros
                            size: self.parent.size
                            pos_hint: {"center_x":0.5, "center_y": 0.58}
                            font_size: int(0.7*self.parent.width/2)
                            line_color_focus: get_color_from_hex('#{}'.format(root.cor_aplicativo))
                            text_color_focus: get_color_from_hex('#{}'.format(root.cor_aplicativo))
                            text:'0'
                            input_filter: 'int'

            # TEXTO DE ALERTA
            MDBoxLayout_Hover:
                orientation: 'vertical'
                md_bg_color: 0,0,0,0
                on_enter: self.cor = self.cor_widgets_hover
                on_leave: self.cor = self.cor_widgets_default
                #pos_hint: 0.5, 0.5
                padding: 10
                canvas.before:
                    Color:
                        rgb: get_color_from_hex('#{}'.format(self.cor))
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
                        radius: [(9.2, 9.2), (9.2, 9.2), (9.2, 9.2), (9.2, 9.2)]

                MDLabel:
                    markup: True
                    text: "[b]Atenção:[/b] Esse valores de acertos e erros serão atribuidos para todos os items que você selecionou."
                    pos_hint: {"center_x":0.5, "center_y": 0.5}
                    font_size: 14
                    color: get_color_from_hex('#{}'.format(root.cor_aplicativo))






